{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "staff-crud-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/staffCrud/StaffCrudWebPart.js",
          "manifest": "./src/webparts/staffCrud/StaffCrudWebPart.manifest.json"
        }
      ]
    },
    "leave-app-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/leaveApp/LeaveAppWebPart.js",
          "manifest": "./src/webparts/leaveApp/LeaveAppWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "StaffCrudWebPartStrings": "lib/webparts/staffCrud/loc/{locale}.js",
    "LeaveAppWebPartStrings": "lib/webparts/leaveApp/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js"
  }
}